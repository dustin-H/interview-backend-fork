{
  "name": "server-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently --kill-others \"npm run pgtyped:watch\" \"npm run dev:watch\"",
    "dev:watch": "nodemon --watch './**/*.ts' --exec 'vite-node' src/index.ts",
    "pgtyped:watch": "npx pgtyped -w -c pgtyped.config.cjs",
    "start-ts": "vite-node src/index.ts",
    "build": "npm run build:ts && npm run build:trpc-api-export",
    "build:ts": "tsc -b tsconfig.json",
    "build:trpc-api-export": "tsup --config trpc-export/tsup.config.ts",
    "lint": "eslint --ext .js,.ts .",
    "test:unit": "jest",
    "test": "jest --config=jest.integration.config.cjs --detectOpenHandles",
    "test-example-client": "vite-node trpc-export/example_client.ts",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "update-redoc": "curl -o public/redoc.js https://cdn.redoc.ly/redoc/latest/bundles/redoc.standalone.js",
    "postinstall": "npm run build"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "keywords": [],
  "author": "brainfood",
  "license": "ISC",
  "dependencies": {
    "@pgtyped/runtime": "^2.1.0",
    "@trpc/client": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "async_hooks": "^1.0.0",
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.3",
    "trpc-openapi": "^1.2.0",
    "tslog": "^4.9.1",
    "zod": "^3.22.1"
  },
  "devDependencies": {
    "@jest/types": "^29.6.1",
    "@pgtyped/cli": "^2.2.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.1.1",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite-node": "^0.34.2",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
